server {
    listen 80;
    charset utf-8;
    root WEBSITE_ROOT_DIR;
    server_name WEBSITE_NAME;

    access_log off;
    error_log WEBSITE_ERROR_LOG_FILE;

    location / {
        try_files $uri $uri/ /index.php?$query_string;
    }

    include conf.d/basic.conf;
    include conf.d/location/php-fpm.conf;
}
